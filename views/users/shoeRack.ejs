<div class="jumbotron text-center">
  <h1>All shoes</h1>
</div>
<div class="container">
  <div class="row">
    <% for (var i = 0; i<shoes.length; i++){%>
      <div class="col-sm-4">
        <h3><a href="/<%=shoes[i].id%>"><%=shoes[i].name%></a></h3>
        <img src=<%=shoes[i].image%>>
        <p><%=shoes[i].colorway%></p>
        <p><%=shoes[i].material%></p>
        <p><%=shoes[i].price%></p>
        <p><%=shoes[i].year%></p>
      </div>
    <%}%>
  </div>
</div>
